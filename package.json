{
  "name": "sitva-gestion-client",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "doc:build": "npx compodoc -p tsconfig.doc.json -d ./documentation --language es-ES --hideGenerator",
    "doc:start": "npx compodoc -s -o -d ./documentation",
    "doc:buld:start": "npm run doc:build & npm run doc:start",
    "mock:api": "json-server --watch mock/api.json",
    "start:mock": "concurrently \"npm run mock:api\" \"npm start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@congreso/ng-admin": "^0.5.0",
    "keycloak-angular": "^13.1.0",
    "keycloak-js": "^21.1.2",
    "ngx-ssrs-reportviewer-v2": "^15.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular/cli": "~15.2.9",
    "@angular/compiler-cli": "^15.2.0",
    "@compodoc/compodoc": "^1.1.21",
    "@types/jasmine": "~4.3.0",
    "concurrently": "^8.2.0",
    "jasmine-core": "~4.5.0",
    "json-server": "^0.17.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
